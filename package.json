    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-modal": "^3.6.1",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-select": "^2.1.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.5",
