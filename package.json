  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.1.2",
    "nodemon": "^1.18.9"
  },
  "scripts": {
    "dev": "next",
    "start-dev": "REACT_APP_VINYL_TRACKER_ENV=staging PORT=4000 react-scripts start",
    "start-staging": "REACT_APP_VINYL_TRACKER_ENV=staging PORT=4000 react-scripts start",
    "build-staging": "REACT_APP_VINYL_TRACKER_ENV=staging react-scripts build",
    "build-live": "REACT_APP_VINYL_TRACKER_ENV=live react-scripts build",
    "lint": "./node_modules/eslint/bin/eslint.js \"./**/*.js\"",
    "deploy": "npm run build && gcloud app deploy",
    "deploy-staging": "npm run build && gcloud app deploy --version=staging",
    "push-google": "git push --all google",
    "build": "next build",
    "release-staging-patch": "./ci/make-tag.sh patch && ./ci/release-staging.sh",
    "release-staging-minor": "./ci/make-tag.sh minor && ./ci/release-staging.sh",
    "release-staging-major": "./ci/make-tag.sh major && ./ci/release-staging.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "cloud-repo-tools": {
    "requiresKeyFile": false,
    "requiresProjectId": false,
    "test": {
      "app": {
        "args": [
          "bin/www"
        ],
        "msg": "Hello from App Engine!"
      }
    }
  }
